<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="together">
    <objectAnimator xmlns:android="http://schemas.android.com/apk/res/android"
        android:valueFrom="0" android:valueTo="360" android:propertyName="rotationX"
        android:duration="500"
    />
    <objectAnimator xmlns:android="http://schemas.android.com/apk/res/android"
        android:valueFrom="0" android:valueTo="360" android:propertyName="rotationY"
        android:duration="250"
        />
    <objectAnimator xmlns:android="http://schemas.android.com/apk/res/android"
        android:valueFrom="0" android:valueTo="360" android:propertyName="rotation"
        android:duration="250"
        />
</set>
